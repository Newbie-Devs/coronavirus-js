function giorno(positivi, guariti, morti, totali) 
{
this.positivi = positivi;
this.guariti = guariti;
this.morti = morti;
this.totali = totali;
}
var 14 = new giorno(17750, 1966, 1441, 21157)

var 15 = new giorno(20603, 2335, 1809, 24747);
