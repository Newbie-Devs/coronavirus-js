var 18 = new giorno(28710, 4025, 2978, 35713);
var 19 = new giorno(33190, 4440, 3405, 41035);
var 20 = new giorno(37860, 5129, 4032, 47021);
var 21 = new giorno(42681, 6072, 4825, 53578);

